{
  "name": "hermit-purple-server",
  "author": "homura.dev@gmail.com",
  "license": "MIT",
  "description": "A Muta GraphQL APIs server",
  "keywords": [
    "muta"
  ],
  "version": "0.1.1-alpha.0",
  "scripts": {
    "start": "node -r module-alias/register dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc --skipLibCheck",
    "generate": "dotenv -- npm -s run generate:schemats && npm -s run generate:nexus && npm -s run generate:docs",
    "generate:docs": "graphql-markdown src/generated/schema.graphql > docs/schema.md",
    "generate:nexus": "ts-node -r tsconfig-paths/register --transpile-only src/server/schema",
    "generate:schemats": "dotenv -- ts-node -r tsconfig-paths/register scripts/schemaToTS.ts",
    "postinstall": "npm -s run generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --no-notify --respawn --transpileOnly src/server/index.ts",
    "dev:sync": "cross-env DEBUG=sync:* ts-node --transpile-only -r tsconfig-paths/register src/impl/sync.ts",
    "sync": "node module-alias/register dist/impl/sync",
    "release:alpha": "standard-version --prerelease alpha",
    "migrate:up": "ts-node --transpile-only -r tsconfig-paths/register src/impl/db/mysql/migration/001.ts up",
    "migrate:down": "ts-node --transpile-only -r tsconfig-paths/register src/impl/db/mysql/migration/001.ts down"
  },
  "_moduleAliases": {
    "@hermit": "dist"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/graphql-relay": "^0.4.11",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/lodash": "^4.14.149",
    "@types/lossless-json": "^1.0.0",
    "axios": "^0.19.2",
    "bignumber.js": "^9.0.0",
    "bluebird": "^3.7.2",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "graphql": "^14.6.0",
    "graphql-query-complexity": "^0.4.1",
    "graphql-relay": "^0.6.0",
    "graphql-yoga": "1.18.3",
    "http-proxy-middleware": "^0.20.0",
    "is-hex": "^1.1.3",
    "knex": "^0.20.10",
    "knex-on-duplicate-update": "^1.1.3",
    "lodash": "^4.17.15",
    "lossless-json": "^1.0.3",
    "module-alias": "^2.2.2",
    "muta-sdk": "^0.7.0-alpha.6",
    "mysql": "^2.18.1",
    "nexus": "0.12.0-rc.13"
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/ws": "6.0.4",
    "cross-env": "^6.0.3",
    "dotenv-cli": "^3.1.0",
    "graphql-markdown": "^5.2.0",
    "jest": "^25.1.0",
    "schemats": "^3.0.3",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.2.1",
    "ts-node": "8.6.2",
    "ts-node-dev": "1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.2"
  }
}
